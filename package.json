{
  "name": "metadot-multisig",
  "description": "multisig subquery for metadot",
  "version": "0.0.4",
  "author": {
    "name": "Ahmed Ali",
    "url": "https://github.com/ahmedali8"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@polkadot/api": "^7",
    "@subql/types": "latest",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.3",
    "@subql/cli": "latest"
  },
  "exports": {
    "chaintypes": "./src/chaintypes.ts"
  },
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -build",
    "clean": "shx rm -rf ./dist ./.data",
    "codegen": "./node_modules/.bin/subql codegen",
    "commit": "git-cz",
    "lint": "yarn lint:ts && yarn prettier:check && yarn typecheck",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install",
    "prepack": "yarn clean && yarn build",
    "start": "docker-compose pull && docker-compose up --remove-orphans",
    "prettier": "prettier --config ./.prettierrc.yaml --ignore-path ./.prettierignore --write \"./src/**/*.{js,json,md,ts,yaml,yml}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml --ignore-path ./.prettierignore \"./src/**/*.{js,json,md,ts,yaml,yml}\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": "github:ahmedali8/metadot-multisig-subql",
  "license": "MIT"
}
